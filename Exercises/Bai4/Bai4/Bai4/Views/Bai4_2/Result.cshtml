
@{
    ViewBag.Title = "Result";
}

<style>
    th, td {
        padding: 5px;
    }
</style>

<table border="1" cellspacing="0" width="500px" style="margin-top: 20px;">
    <tr>
        <td>Chủ hộ:</td>
        <td>@ViewBag.name</td>
    </tr>
    <tr>
        <td>Số điện tiêu thụ:</td>
        <td>@ViewBag.dien</td>
    </tr>
    <tr>
        <td>Tiền phải trả:</td>
        <td>@ViewBag.tien</td>
    </tr>
</table>